{
  "apiKey": 93,
  "type": "request",
  "listeners": ["broker"],
  "name": "DisklessCommitRequest",
  "validVersions": "0",
  "flexibleVersions": "0+",
  "fields": [
    { "name": "ObjectName", "type": "string", "versions": "0+",
      "about": "The object name for the diskless commit operation." },
    { "name": "Topics", "type": "[]TopicDisklessCommitData", "versions": "0+",
      "about": "Each topic to commit data for.", "fields": [
      { "name": "TopicId", "type": "uuid", "versions": "0+", "mapKey": true,
        "about": "The unique topic ID." },
      { "name": "Partitions", "type": "[]PartitionDisklessCommitData", "versions": "0+",
        "about": "Each partition to commit data for.", "fields": [
        { "name": "Partition", "type": "int32", "versions": "0+",
          "about": "The partition index." },
        { "name": "Batches", "type": "[]BatchDisklessCommitData", "versions": "0+",
          "about": "Each batch to commit.", "fields": [
          { "name": "Magic", "type": "int8", "versions": "0+",
            "about": "The magic number for the batch format." },
          { "name": "ByteOffset", "type": "int32", "versions": "0+",
