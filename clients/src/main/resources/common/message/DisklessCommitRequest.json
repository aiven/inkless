          { "name": "ProducerEpoch", "type": "int16", "versions": "0+",
            "about": "" },
          { "name": "BaseSequence", "type": "int32", "versions": "0+",
            "about": "" },
          { "name": "LastSequence", "type": "int32", "versions": "0+",
            "about": "" }
        ]}
      ]}
    ]}
  ]
